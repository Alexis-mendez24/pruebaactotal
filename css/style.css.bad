	font-weight:700;
	text-align:center;
}
.btn-primary {
	border-radius:0;
	border:2px solid #fff;
	background:transparent;
	padding:10px 30px;
	text-transform:uppercase;
	transition:all ease-in-out 0.3s;
	-webkit-transition:all ease-in-out 0.3s;
