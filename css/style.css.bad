/*iPhone 6 landscape*/
@media only screen and (max-device-width: 667px) and (max-device-height: 375px) and (orientation : landscape) { 
    .carousel {
        height: 375px;
    }

    .carousel .item {
        width: 559px;
        height: 375px;
    }
    .arrow-next-back {
        position: absolute;
        display: block;
        cursor: pointer;
        opacity: 0.6;
        z-index: 5;
        width: 100px;
        height: 100px;
        top: 140px;
    }
    .carousel-indicators {
        position: absolute;
        bottom: 20px;
    }
}
