/* SCROLL ICON*/
@-webkit-keyframes ani-mouse {
	0% {
	opacity: 1;
	top: -10px;
	}
	15% {
	opacity: 1;
	top: 20%;
	}
	50% {
	opacity: 0;
	top: 20%;
	}
	100% {
	opacity: 0;
	top: -10px;
	}
}
@-moz-keyframes ani-mouse {
	0% {
	opacity: 1;
	top: -10px;
	}
	15% {
	opacity: 1;
	top: 20%;
	}
	50% {
	opacity: 0;
	top: 20%;
	}
	100% {
	opacity: 0;
	top: -10px;
	}
}
@keyframes ani-mouse {
	0% {
	opacity: 1;
	top: -10px;
	}
	15% {
	opacity: 1;
	top: 20%;
	}
	50% {
	opacity: 0;
	top: 20%;
	}
	100% {
	opacity: 0;
	top: -10px;
	}
}
 .scroll-btn {
	display: block;
	position: fixed;
	right: 50px;
	text-align: center;
	bottom: 0;
	z-index: 999;
}


.scroll-btn > *:hover,
.scroll-btn > *:focus,
.scroll-btn > *.active {
	color: #ffffff;
}
.scroll-btn > *:hover,
.scroll-btn > *:focus,
.scroll-btn > *:active,
.scroll-btn > *.active {
	opacity: 0.8;
	filter: alpha(opacity=80);
}
.scroll-btn .mouse {
	position: relative;
	display: block;
	width: 55px;
	height: 55px;
	margin: 0 auto 20px;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	border: 1px solid white;
	border-radius: 50%;
}
.scroll-btn .mouse span {
	position: absolute;
	display: block;
	width: 55px;
	height: 55px;
	top:0;
	left:-1px;
	-webkit-animation: ani-mouse 2.5s linear infinite;
	-moz-animation: ani-mouse 2.5s linear infinite;
	animation: ani-mouse 2.5s linear infinite;
}
.scroll-btn .mouse span::after {
	content: url(../img/down-arrow.svg);
	height: 55px;
	width: 55px;
}

