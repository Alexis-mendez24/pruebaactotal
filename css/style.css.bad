/* MAIN SECTION*/
section.main {
	height:100vh;
	text-align:center;
	background-size:cover;
}
section.main h2 {
    position: absolute;
    top: 27%;
    width: 100%;
    text-align: center;
    text-transform: uppercase;
    font-size: 16px;
    margin-top: 10px;
    font-weight: bold;
